<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile_page">

    <LinearLayout android:id="@+id/linearColumnvectorOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="center"
            android:layout_height="70dp"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp"
            android:elevation="5dp"
            style="@style/groupStyleblue_gray_800_02">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/header_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/imageArrowBack"
                    android:layout_width="16dp"
                    android:layout_height="30dp"
                    android:layout_gravity="top"
                    android:layout_marginStart="24dp"
                    android:src="@drawable/img_arrow"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrow" />

                <TextView android:ellipsize="end"
                    android:id="@+id/txtProfile"
                    android:layout_gravity="center_vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    style="@style/txtPoppinsbold20White"
                    android:text="@string/lbl_profile"
                    android:layout_height="wrap_content" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearProfilepage"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="7dp" android:paddingTop="7dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnwelcomede"
                android:layout_height="wrap_content"
                android:layout_marginEnd="18dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="14dp"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWelcomedeon"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="28dp"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_welcome_deon_your"
                    style="@style/txtInterregular12" />
                <FrameLayout android:id="@+id/frameStackvectoreigh"
                    android:layout_height="126dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="7dp" android:layout_width="310dp">

                    <ImageView
                        android:id="@+id/imageVectoreightOne"
                        android:layout_width="185dp"
                        android:layout_height="107dp"
                        android:layout_gravity="bottom|end"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_8"
                        />

                    <LinearLayout android:id="@+id/linearColumnprice"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="21dp"
                        android:layout_marginEnd="186dp"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice2"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_position_p_l"
                            style="@style/txtInterregular12_1" />
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnMakeADeposit"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="14dp"
                            android:layout_marginTop="22dp"
                            android:layout_width="match_parent"
                            android:paddingBottom="7dp"
                            android:paddingEnd="10dp"
                            android:paddingStart="10dp"
                            android:paddingTop="7dp"
                            android:singleLine="true"
                            android:text="@string/lbl_make_a_deposit"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_1" />
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice1"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_140203_38"
                        style="@style/txtIntermedium24" />
                </FrameLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPortfolio"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="8dp"
                android:singleLine="true"
                android:text="@string/lbl_portfolio"
                style="@style/txtPoppinssemibold16" />
<!--            <androidx.recyclerview.widget.RecyclerView-->
<!--                android:gravity="end"-->
<!--                android:id="@+id/recyclerUserprofile"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:layout_marginStart="20dp"-->
<!--                android:layout_marginTop="18dp"-->
<!--                android:layout_width="match_parent"-->
<!--                android:orientation="horizontal"-->
<!--                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"-->
<!--                itemSpace="17dp" tools:itemCount="2" tools:listitem="@layout/row_userprofile"/>-->
            <!--portfolio-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_marginStart="20dp"
                >

                <LinearLayout
                    android:id="@+id/linearUserprofile"
                    style="@style/groupStylecornerRadius_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:layout_marginEnd="10dp"
                    >

                    <LinearLayout
                        android:id="@+id/linearToken"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageUserImage"
                            android:layout_width="@dimen/_40pxh"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_gravity="top"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:scaleType="centerCrop"
                            android:src="@drawable/img_icon_near" />

                        <LinearLayout
                            android:id="@+id/linearColumnappname"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:gravity="start"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtAppName"
                                style="@style/txtPoppinssemibold16_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:letterSpacing="0.01"
                                android:lineSpacingExtra="@dimen/_8pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_apple" />

                            <TextView
                                android:id="@+id/txtCompanyName"
                                style="@style/txtRobotoregular12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:ellipsize="end"
                                android:letterSpacing="0.02"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_apple_inc" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowprice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtPrice5"
                            style="@style/txtPoppinssemibold16_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_198_24" />

                        <LinearLayout
                            android:id="@+id/linearRowdiscountperc"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_55pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageDiscountImage"
                                android:layout_width="@dimen/_12pxh"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_fill_system_arrow_up_fill" />

                            <TextView
                                android:id="@+id/txtDiscountPercent"
                                style="@style/txtRobotoregular12_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:letterSpacing="0.02"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_2_5" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearUserprofile2"
                    style="@style/groupStylecornerRadius_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingBottom="@dimen/_11pxv">

                    <LinearLayout
                        android:id="@+id/linearToken2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageUserImage2"
                            android:layout_width="@dimen/_40pxh"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_gravity="top"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:scaleType="centerCrop"
                            android:src="@drawable/img_icon_near_40x40" />

                        <LinearLayout
                            android:id="@+id/linearColumnappname2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:gravity="start"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtAppName2"
                                style="@style/txtPoppinssemibold16_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:letterSpacing="0.01"
                                android:lineSpacingExtra="@dimen/_8pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_lyft" />

                            <TextView
                                android:id="@+id/txtCompanyName2"
                                style="@style/txtRobotoregular12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:ellipsize="end"
                                android:letterSpacing="0.02"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_lyft_inc" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowprice2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtPrice3"
                            style="@style/txtPoppinssemibold16_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_410_01" />

                        <LinearLayout
                            android:id="@+id/linearRowdiscountperc2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_55pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageDiscountImage2"
                                android:layout_width="@dimen/_12pxh"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_fill_system_arrow_up_fill" />

                            <TextView
                                android:id="@+id/txtDiscountPercent2"
                                style="@style/txtRobotoregular12_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:letterSpacing="0.02"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_1_12" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!--portfolio end-->

            <TextView android:ellipsize="end"
                android:id="@+id/txtPendinglist"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="11dp"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="8dp"
                android:singleLine="true"
                android:text="@string/lbl_pending_list"
                style="@style/txtPoppinssemibold16" />
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerStockinfo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="6dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="9dp"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="11dp" tools:itemCount="2" tools:listitem="@layout/row_stockinfo"/>
            <TextView android:ellipsize="end" android:id="@+id/txtToday"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="11dp"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="8dp"
                android:singleLine="true"
                android:text="@string/msg_today_transactions"
                style="@style/txtPoppinssemibold16" />
            <LinearLayout android:id="@+id/linearTransactions"
                android:layout_height="wrap_content"
                android:layout_marginEnd="12dp"
                android:layout_marginStart="12dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="13dp"
                android:paddingEnd="8dp"
                android:paddingStart="8dp"
                android:paddingTop="13dp" style="@style/groupStylecornerRadius_3">
                <FrameLayout android:id="@+id/frameLogo"
                    android:layout_height="40dp"
                    android:layout_marginBottom="6dp"
                    android:layout_marginTop="6dp"
                    android:layout_width="41dp"
                    android:paddingBottom="10dp"
                    android:paddingEnd="3dp"
                    android:paddingStart="3dp"
                    android:paddingTop="10dp" style="@style/groupStyleorange_100cornerRadius">
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="end"
                        android:layout_height="19dp"
                        android:layout_width="32dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle_26"
                        />
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnbuy"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameStackbuy"
                        android:layout_height="33dp" android:layout_width="115dp">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBuy"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_buy"
                            style="@style/txtLexendDecabold16" />
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNike"
                            android:layout_gravity="top|end"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="37dp"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_nike"
                            style="@style/txtPoppinsregular16" />
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txt10816611xten"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_108_16611_x_10"
                            style="@style/txtKhularegular10" />
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1082240216twent"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/msg_108_22402_16_25"
                        style="@style/txtKhularegular10" />
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumn0fiftyfou"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtPrice4"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_8_03"
                        style="@style/txtIntersemibold13" />
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txt0fiftyfour"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_0_54"
                        style="@style/txtInterregular8" />
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtMore"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="8dp"
                android:singleLine="true"
                android:text="@string/lbl_more"
                style="@style/txtPoppinssemibold16_3" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>